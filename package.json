{
  "name": "earningsjr",
  "private": true,
  "version": "0.0.1",
  "packageManager": "pnpm@9.0.0",
  "workspaces": [
    "apps/*",
    "workers/*"
  ],
  "scripts": {
    "build": "echo \"Use per-package builds\"",
    "dev": "echo \"Use per-package dev scripts\"",
    "dev:api": "pnpm -C workers/api dev",
    "deploy:api": "pnpm -C workers/api deploy",
    "typecheck:api": "pnpm -C workers/api typecheck",
    "dev:web": "pnpm -C apps/web dev",
    "build:web": "pnpm -C apps/web build"
  }
}

